  "scripts": {
    "fuego-publish": "node ./tools/workflows/build-to-publish.js",
    "postinstall": "ngcc --properties es2020 browser module main",
    "merge:components": "cat dist/packages/fuego-ui-styles/*.css > dist/packages/fuego-ui-styles/components.css",
    "postmerge:components": "prejss-cli dist/packages/fuego-ui-styles/components.css --format commonjs",
    "prebuild:fuego-styles": "rm -rf dist/packages/fuego-ui-styles",
    "build:fuego-styles": "postcss --config packages/fuego-styles packages/fuego-styles/src/components/*.css src --dir dist/packages/fuego-ui-styles && npm run merge:components && nx build fuego-styles"
  },
